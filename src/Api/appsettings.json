{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Connections": {
    "ContextDatabase": {
      "Host": "localhost",
      "User": "root",
      "Password": "admin"
    },
    "Redis": {
      "Host": "localhost",
      "Port": 6379,
      "DefaultDatabase": 3,
      "AuthToken": "",
      "UseSsl": false,
      "SlidingExpirationInHours": 24,
      "KeyPrefix": "certificateservice",
      "ClearRedisOnStartup": true
    },
    "ElasticSearch": {
      "Connection": "http://localhost:9200",
      "Index": "search_certificates_logs"
    }
  }
}
